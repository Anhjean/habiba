<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <template id="assets_frontend" active="True" inherit_id="website.assets_frontend" name="habiba_custom assets_frontend">
        <xpath expr="//link[last()]" position="after">

            <meta name="viewport" content="width=device-width, initial-scale=1"/>
            <link rel="preconnect" href="https://fonts.googleapis.com"/>
            <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin="true"/>
            <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@100;400;500;800&amp;display=swap" rel="stylesheet"/>
            <link rel="preconnect" href="https://fonts.googleapis.com"/>
            <link href="https://fonts.googleapis.com/css2?family=Cantarell&amp;family=Raleway:wght@100&amp;display=swap" rel="stylesheet" />

            <link rel="stylesheet" href="https://unpkg.com/swiper@8/swiper-bundle.min.css"/>

            <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
            <link rel="stylesheet" href="/habiba_custom/static/src/css/shop.css"/>
            <link rel="stylesheet" href="/habiba_custom/static/src/css/main.css"/>
            <link rel="stylesheet" href="/habiba_custom/static/src/css/custom.css"/>
            <link rel="stylesheet" href="/habiba_custom/static/src/css/responsibility.css"/>
            <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous"/>
            <link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.10.0/css/all.css" integrity="sha384-AYmEC3Yw5cVb3ZcuHtOA93w35dYTsvhLPVnYs9eStHfGJvOvKxVfELGroGkvsg+p" crossorigin="anonymous"/>

            <link rel="stylesheet" type='text/scss' href="/habiba_custom/static/src/scss/footer.scss"/>
            <link rel="stylesheet" type='text/scss' href="/habiba_custom/static/src/scss/auth.scss"/>
            <link rel="stylesheet" type='text/scss' href="/habiba_custom/static/src/scss/product.scss"/>
            <link rel="stylesheet" type='text/scss' href="/habiba_custom/static/src/scss/shop.scss"/>
            <link rel="stylesheet" type='text/scss' href="/habiba_custom/static/src/scss/cart.scss"/>

        </xpath>
        <xpath expr="script[last()]" position="after">
            <script src="https://unpkg.com/swiper@8/swiper-bundle.min.js"></script>

            <script src="/habiba_custom/static/src/js/dynamic_product_popup.js"/>

            <!-- Start Added by Hoanglv -->

            <link rel="stylesheet" href="/habiba_custom/static/lib/owlcarousel/owl.carousel.min.css"/>
            <link rel="stylesheet" href="/habiba_custom/static/lib/owlcarousel/owl.theme.default.min.css"/>
            <script src="/habiba_custom/static/lib/owlcarousel/owl.carousel.js"/>
            <!-- <script src="//cdnjs.cloudflare.com/ajax/libs/timepicker/1.3.5/jquery.timepicker.min.js"></script> -->

            <script src="/habiba_custom/static/src/js/swipers.js"/>
            <script src="/habiba_custom/static/src/js/website_sale_widget.js"/>
            <script src="/habiba_custom/static/src/js/delivery_pickup.js"/>
            <script src="/habiba_custom/static/src/js/scripts.js"/>
            <script src="/habiba_custom/static/src/js/shop.js"/>
            <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>

            <!-- End Added by Hoanglv -->


        </xpath>
    </template>

    <template id="layout" active="True" inherit_id="web.layout">
        <xpath expr="//link" position="after">
            <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet"/>
        </xpath>
    </template>

</odoo>

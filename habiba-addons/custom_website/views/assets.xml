<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <template id="assets_frontend" inherit_id="web.assets_frontend">
    <xpath expr="//link[last()]" position="after">
      <link href="/custom_website/static/src/scss/header.scss"
      rel="stylesheet" />
      <link href="/custom_website/static/src/scss/jquery.offcanvas.min.css"
      rel="stylesheet" />
    </xpath>
    <xpath expr="//script[last()]" position="after">
      <script src="/custom_website/static/lib/js/velocity.js" />
      <script src="/custom_website/static/lib/js/offcanvas.js" />
      <script src="/custom_website/static/src/js/cart_popup.js" />
      <script src="/custom_website/static/src/js/add_dynamic.js" />
      <script src="/custom_website/static/src/js/product_modal.js" />
    </xpath>
  </template>
</odoo>
